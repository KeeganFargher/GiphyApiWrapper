<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <Authors>Keegan Fargher</Authors>
    <PackageId>GiphyApiWrapper</PackageId>
    <Version>1.1.1</Version>
    <Description>An API wrapper that makes it easy to consume Giphy endpoints.</Description>
    <RepositoryUrl>https://github.com/KeeganFargher/GiphyApiWrapper</RepositoryUrl>
    <PackageTags>giphy api keegan fargher standard dotnet gif search images jif wrapper</PackageTags>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/KeeganFargher/GiphyApiWrapper</PackageProjectUrl>
    <PackageReleaseNotes>Fixed an issue where methods were using the wrong object</PackageReleaseNotes>
    <PackageIconUrl>https://giphy.com/static/img/giphy_logo_square_social.png</PackageIconUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
  </ItemGroup>
  
</Project>
